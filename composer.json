{
    "name": "aedart/athenaeum-console",
    "description": "Offers a way to register commands and schedules via configuration",
    "keywords": [
        "Athenaeum",
        "Console",
        "Command",
        "Schedule"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "illuminate/cache": "^7.0",
        "illuminate/console": "^7.0",
        "dragonmantank/cron-expression": "^2.3",
        "aedart/athenaeum-contracts": "dev-master",
        "aedart/athenaeum-support": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Console\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
