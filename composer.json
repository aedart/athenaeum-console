{
    "name": "aedart/athenaeum-console",
    "description": "Offers a way to register commands and schedules via configuration",
    "keywords": ["Athenaeum", "Console", "Command", "Schedule"],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "aedart/athenaeum-contracts": "^6.0.2",
        "aedart/athenaeum-support": "^6.0.2",
        "dragonmantank/cron-expression": "^3.3",
        "illuminate/cache": "^v9.7.0",
        "illuminate/console": "^v9.7.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Console\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
